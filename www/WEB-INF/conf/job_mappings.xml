<matcher uri="admin/j_module" command-name="j-manage-jobs">
    <bind result="success" view="html-view">
        <parameter name="xsl" value="J_DEPARTMENTS_XSL"/>
    </bind>
    <bind result="department" view="html-view">
        <parameter name="xsl" value="J_DEPARTMENT_XSL"/>
    </bind>
    <bind result="vacancy" view="html-view">
        <parameter name="xsl" value="J_VACANCY_XSL"/>
    </bind>
    <bind result="applicant" view="html-view">
        <parameter name="xsl" value="J_APPLICANT_XSL"/>
    </bind>
    <bind result="application_form" view="html-view">
        <parameter name="xsl" value="J_APPLICATION_FORM_XSL"/>
    </bind>
</matcher>

<matcher uri="admin/j_module_xml" command-name="j-manage-jobs">
    <bind result="success" view="xml-view"/>
    <bind result="vacancy" view="xml-view"/>
    <bind result="applicant" view="xml-view"/>
    <bind result="application_form" view="xml-view"/>
</matcher>

<matcher uri="admin/field_repository" command-name="j-field-repository">
    <bind result="field-type-chooser" view="html-view">
        <parameter name="xsl" value="J_FIELD_CHOOSER_XSL"/>
    </bind>
    <bind result="field-type-list" view="html-view">
        <parameter name="xsl" value="J_FIELD_REPOSITORY_XSL"/>
    </bind>
    <bind result="field-type-details" view="html-view">
        <parameter name="xsl" value="J_FIELD_DETAILS_XSL"/>
    </bind>
</matcher>

<matcher uri="admin/field_repository_xml" command-name="j-field-repository">
    <bind result="field-type-chooser" view="xml-view"/>
    <bind result="field-type-list" view="xml-view"/>
    <bind result="field-type-details" view="xml-view"/>
</matcher>

