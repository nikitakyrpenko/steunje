        <!--
            Default bindings for result names "failure" and "denied":
                <bind result="failure" view="resource-not-found-view"/>
                <bind result="denied" view="access-denied-view"/>
         -->
         
            <matcher uri="/pl_link" command-name="pl-decode-link">
                <bind result="success" view="html-view">
                    <parameter name="xsl" value="PL_PARAMETRIZED_LINK"/>
                </bind>                
                <bind result="failure" view="resource-not-found-view"/>
                <bind result="denied"  view="html-view"><parameter name="xsl" value="LOGIN_XSL"/></bind>
            </matcher>
         
         
