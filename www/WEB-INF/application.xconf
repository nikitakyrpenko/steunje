                <bind result="success" view="html-view"><parameter name="xsl" value="SITE_XSL"/></bind>
            </matcher>
			
			<matcher uri="/@lang/*.htm*" command-name="get-page-command">
				<parameter name="langCode" value="{1}"/>
                <parameter name="filename" value="{2}.html"/>
                <bind result="success" view="html-view"><parameter name="xsl" value="SITE_XSL"/></bind>
            </matcher>
			<matcher uri="/@lang/*" command-name="get-page-command">
				<parameter name="langCode" value="{1}"/>
                <parameter name="filename" value="{2}.html"/>
                <bind result="success" view="html-view"><parameter name="xsl" value="SITE_XSL"/></bind>
            </matcher>
			
			
			
			
			<matcher uri="/admin/@lang/*.xml" command-name="get-editable-page-command">
